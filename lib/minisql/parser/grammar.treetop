module MiniSQL

grammar SQL do

  include CreateTableExp
  include DropTableExp
  include CreateIndexExp
  include DropIndexExp
  include SelectExp

  rule expression do
      create_table_exp / 
      drop_table_exp / 
      create_index_exp / 
      drop_index_exp / 
      select_exp
  end

end

end

